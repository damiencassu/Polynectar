{
"name": "Polynectar",
"manifest_version": 2,
"version": "0.0.1",
"description": "",
"permissions": 
	[
		"webRequest", 
		"webRequestBlocking", 
		"*://*/*"
	],
"options_page": "html/options.html",
"background": {
	"scripts": ["js/background.js"],
	"persistent": true
},
"browser_action":
	{
		"default_icon": "img/icone.png",
		"default_popup": "html/popup.html"
	}
}
